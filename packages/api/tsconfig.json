{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,              /* <- ESSA LINHA CORRIGE O ERRO */
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",           /* <- IMPORTANTE PARA DEPENDÃŠNCIAS NODE */
    "resolveJsonModule": true
  }
}